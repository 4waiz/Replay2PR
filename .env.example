GEMINI_API_KEY=
# Optional: override Gemini model names
GEMINI_MODEL_FLASH=gemini-3-flash
GEMINI_MODEL_PRO=gemini-3-pro
# Base URL for Playwright to target
BASE_URL=http://localhost:3000
# Max upload size in MB
MAX_UPLOAD_MB=50
# Max concurrent jobs (Playwright runs)
MAX_CONCURRENT_JOBS=1
# Force mock Gemini responses (true/false)
USE_MOCK_GEMINI=false
